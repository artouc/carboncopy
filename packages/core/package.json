{
    "name": "@osaxyz/carboncopy",
    "version": "0.1.0",
    "description": "複雑な要素を含むHTMLを正確にPDFにレンダリングするためのクライアントサイドライブラリ",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "vitest",
        "test:run": "vitest run",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "pdf",
        "html",
        "html-to-pdf",
        "converter",
        "print",
        "invoice",
        "report",
        "japanese"
    ],
    "author": "Arata Ouchi (Original SIN Architecture)",
    "license": "MIT",
    "homepage": "https://carboncopy.dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/artouc/carboncopy.git",
        "directory": "packages/core"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.0",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-typescript": "^12.1.0",
        "@types/node": "^22.0.0",
        "@types/opentype.js": "^1.3.8",
        "@types/pako": "^2.0.4",
        "rollup": "^4.28.0",
        "rollup-plugin-dts": "^6.1.0",
        "tslib": "^2.8.0",
        "tsx": "^4.21.0",
        "typescript": "^5.7.0",
        "vitest": "^2.1.0"
    },
    "dependencies": {
        "opentype.js": "^1.3.4",
        "pako": "^2.1.0"
    }
}
